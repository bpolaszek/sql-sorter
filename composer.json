{
    "name": "bentools/sql-sorter",
    "description": "Use the power of SQL to sort data.",
    "type": "library",
    "require": {
        "bentools/simple-dbal": "^0.6.0",
        "bentools/where": "~1.1",
        "robinvdvleuten/ulid": "~2.0",
        "thecodingmachine/safe": "~0.1.8"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "BenTools\\SqlSorter\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
          "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "license": "MIT",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
